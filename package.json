{
	"name": "ui5i18n",
	"displayName": "ui5 i18n",
	"description": "Provides auto completion in xml files for i18n properties defined in your project",
	"version": "0.14.2",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/ui5.yaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "properties",
				"path": "./snippets/snippets.json"
			}
		],
		"configuration": {
			"title": "ui5i18n",
			"properties": {
				"ui5i18n.i18nGlobPattern": {
					"type": "string",
					"default": "{webapp,src}/**/i18n.properties",
					"description": "Glob pattern for the i18n properties of your ui5 app"
				}
			}
		}
	},
	"publisher": "majany",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/majany/ui5i18n.git"
	},
	"dependencies": {
		"i18nparser": "github:majany/i18nparser"
	}
}
